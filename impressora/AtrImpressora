class impressora
{
	constructor(marca, modelo, local, id)
	{
		this.marca=marca;
		this.modelo=modelo;
		this.local=local;
		this.ip=ip;
		
	}
	
	toString ()
	{
		console.log(this.marca+" | "+this.modelo+" | "+this.local+" | "+this.ip);
	}
}

